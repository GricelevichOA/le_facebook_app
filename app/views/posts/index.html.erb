<h1>Hello</h1>

<div class="container">
	<p><%= render 'form' %></p>
			<% @posts.each do |post| %>
			<p><%= post.content %></p>			
			<p>Written by: <%= post.user.email %></p>
			<p>
				<%= link_to 'Show', post_path(post) %>
				<%= link_to 'Edit', edit_post_path(post) %>
				<%= link_to 'Delete', post_path(post), method: :delete, data: { confirm: 'Are you sure?' } %>
			</p> 
			-----------------------------------------
		<% end %>
</div>